{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Manrope & Poppins (Sans/Sans) (Professional)",
	"personality": [
		"professional"
	],
	"keywords": [
		"sans-serif",
		"grotesque",
		"modern",
		"clean",
		"neutral",
		"geometric",
		"magazine",
		"light"
	],
	"settings": {
		"typography": {
			"fontFamilies": [
