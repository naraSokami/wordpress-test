						{
							"src": "https://fonts.wp.com/s/urbanist/v15/L0xtDF02iFML4hGCyMqgdyNEf6or5L2WA133VZmqacG1K4S1.woff2",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontFamily": "Urbanist"
						},
						{
							"src": "https://fonts.wp.com/s/urbanist/v15/L0xjDF02iFML4hGCyOCpRdycFsGxSrqD-R4fE5OrS8SlKw.woff2",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontFamily": "Urbanist"
						},
						{
							"src": "https://fonts.wp.com/s/urbanist/v15/L0xtDF02iFML4hGCyMqgdyNEf6or5L2WA13FVZmqacG1K4S1.woff2",
							"fontWeight": "500",
							"fontStyle": "italic",
							"fontFamily": "Urbanist"
						},
						{
							"src": "https://fonts.wp.com/s/urbanist/v15/L0xjDF02iFML4hGCyOCpRdycFsGxSrqDFRkfE5OrS8SlKw.woff2",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontFamily": "Urbanist"
						},
						{
							"src": "https://fonts.wp.com/s/urbanist/v15/L0xtDF02iFML4hGCyMqgdyNEf6or5L2WA10pUpmqacG1K4S1.woff2",
							"fontWeight": "600",
							"fontStyle": "italic",
							"fontFamily": "Urbanist"
						}
					],
					"fontFamily": "\"Urbanist\", sans-serif",
					"name": "Urbanist",
					"slug": "urbanist"
				},
				{
					"fontFace": [
						{
							"src": "https://fonts.wp.com/s/poppins/v23/pxiByp8kv8JHgFVrLDz8V15vFP-KUEg.woff2",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontFamily": "Poppins"
						},
						{
