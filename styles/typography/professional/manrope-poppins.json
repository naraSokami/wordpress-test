							"fontWeight": "300",
							"fontStyle": "normal",
							"fontFamily": "Manrope"
						},
						{
							"src": "https://fonts.wp.com/s/manrope/v15/xn7_YHE41ni1AdIRqAuZuw1Bx9mbZk79FO_A87jxeN7B.woff2",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontFamily": "Manrope"
						},
						{
							"src": "https://fonts.wp.com/s/manrope/v15/xn7_YHE41ni1AdIRqAuZuw1Bx9mbZk7PFO_A87jxeN7B.woff2",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontFamily": "Manrope"
						},
						{
							"src": "https://fonts.wp.com/s/manrope/v15/xn7_YHE41ni1AdIRqAuZuw1Bx9mbZk4jE-_A87jxeN7B.woff2",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontFamily": "Manrope"
						},
						{
							"src": "https://fonts.wp.com/s/manrope/v15/xn7_YHE41ni1AdIRqAuZuw1Bx9mbZk4aE-_A87jxeN7B.woff2",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontFamily": "Manrope"
						},
						{
							"src": "https://fonts.wp.com/s/manrope/v15/xn7_YHE41ni1AdIRqAuZuw1Bx9mbZk59E-_A87jxeN7B.woff2",
							"fontWeight": "800",
							"fontStyle": "normal",
							"fontFamily": "Manrope"
						}
					],
					"fontFamily": "\"Manrope\", sans-serif",
					"name": "Manrope",
					"slug": "manrope"
				},
				{
					"fontFace": [
						{
							"src": "https://fonts.wp.com/s/poppins/v23/pxiByp8kv8JHgFVrLDz8V15vFP-KUEg.woff2",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontFamily": "Poppins"
						},
						{
							"src": "https://fonts.wp.com/s/poppins/v23/pxiDyp8kv8JHgFVrJJLm21llFd2PQEhcqw.woff2",
							"fontWeight": "300",
							"fontStyle": "italic",
							"fontFamily": "Poppins"
						},
						{
							"src": "https://fonts.wp.com/s/poppins/v23/pxiEyp8kv8JHgFVrFJXUc1NECPY.woff2",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontFamily": "Poppins"
						},
						{
