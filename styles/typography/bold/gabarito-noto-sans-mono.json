{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Gabarito & Noto Sans Mono (Sans/Mono) (Bold)",
	"personality": [
		"bold"
	],
	"keywords": [
		"monospace",
		"uppercase",
		"geometric",
		"dramatic",
		"wide",
		"tech",
		"edgy",
		"typewriter",
		"minimal"
	],
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Gabarito",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/gabarito/Gabarito-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "\"Gabarito\", sans-serif",
					"name": "Gabarito",
					"slug": "gabarito"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Noto Sans Mono",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 800",
							"src": [
								"file:./assets/fonts/noto-sans-mono/NotoSansMono-VariableFont_wdth_wght.woff2"
							]
						}
					],
					"fontFamily": "\"Noto Sans Mono\", monospace",
					"name": "Noto Sans Mono",
					"slug": "noto-sans-mono"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "14px",
					"slug": "small",
					"fluid": {
						"min": "13px",
						"max": "14px"
					}
				},
				{
					"name": "Medium",
					"size": "18px",
					"slug": "medium",
					"fluid": {
						"min": "16px",
						"max": "18px"
					}
				},
				{
					"name": "Large",
					"size": "32px",
					"slug": "large",
					"fluid": {
						"min": "24px",
						"max": "32px"
					}
				},
				{
					"name": "Extra Large",
					"size": "56px",
					"slug": "x-large",
					"fluid": {
						"min": "40px",
						"max": "56px"
					}
				},
				{
					"name": "2X Large",
					"size": "72px",
					"slug": "xx-large",
					"fluid": {
						"min": "44px",
						"max": "72px"
					}
				},
				{
					"name": "3X Large",
					"size": "170px",
					"slug": "xxx-large",
					"fluid": {
						"min": "40px",
						"max": "170px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontSize": "15px",
					"fontFamily": "var(--wp--preset--font-family--noto-sans-mono)",
					"fontWeight": "400",
					"letterSpacing": "-0.01em",
					"textTransform": "none",
					"lineHeight": {
						"ref": "styles.blocks.core/navigation.typography.lineHeight"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "15px",
					"fontFamily": "var(--wp--preset--font-family--noto-sans-mono)",
					"fontWeight": "400",
					"letterSpacing": "-0.01em",
					"lineHeight": "1.4"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "15px",
					"fontFamily": "var(--wp--preset--font-family--noto-sans-mono)",
					"fontWeight": "400",
					"letterSpacing": "-0.01em"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontSize": "15px",
					"fontFamily": "var(--wp--preset--font-family--noto-sans-mono)",
					"fontWeight": "400",
					"letterSpacing": "-0.01em"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gabarito)",
					"fontWeight": "600",
					"lineHeight": "1",
					"textTransform": "uppercase",
					"letterSpacing": "-0.02em"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "-0.01em"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"letterSpacing": "-0.01em"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"letterSpacing": "-0.01em"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"letterSpacing": "-0.01em"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--noto-sans-mono)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"lineHeight": "1.9",
			"letterSpacing": "-0.01em"
		}
	}
}