{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Abel & Ibarra Real Nova (Sans/Serif) (Sophisticated)",
	"personality": [
		"sophisticated"
	],
	"keywords": [
		"sans-serif",
		"simple",
		"clean",
		"minimal",
		"light",
		"tech",
		"futuristic",
		"refined",
		"narrow",
		"thin"
	],
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"src": "https://fonts.wp.com/s/abel/v18/MwQ5bhbm2POE6V1LPJp6qGI.woff2",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontFamily": "Abel"
						}
					],
					"fontFamily": "\"Abel\", sans-serif",
					"name": "Abel",
					"slug": "abel"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Ibarra Real Nova",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 800",
							"src": [
								"file:./assets/fonts/ibarra-real-nova/IbarraRealNova-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "\"Ibarra Real Nova\", serif",
					"name": "Ibarra Real Nova",
					"slug": "ibarra-real-nova"
				},
				{
					"fontFace": [
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxhHiqOu8IVPmnRc6SY1KXhnF_Y8ajfCLrEXFh2reU.woff2",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxhHiqOu8IVPmnRc6SY1KXhnF_Y8fbfCLrEXFh2reU.woff2",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxhHiqOu8IVPmnRc6SY1KXhnF_Y8cTfCLrEXFh2reU.woff2",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxhHiqOu8IVPmnRc6SY1KXhnF_Y8SjYCLrEXFh2reU.woff2",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxjHiqOu8IVPmn7epZnDMyKBvHf5D6cvvzOXXpzveWxBw.woff2",
							"fontWeight": "300",
							"fontStyle": "italic",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxjHiqOu8IVPmn7epZnDMyKBvHf5D6c4PzOXXpzveWxBw.woff2",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxjHiqOu8IVPmn7epZnDMyKBvHf5D6c0vzOXXpzveWxBw.woff2",
							"fontWeight": "500",
							"fontStyle": "italic",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxjHiqOu8IVPmn7epZnDMyKBvHf5D6cPvvOXXpzveWxBw.woff2",
							"fontWeight": "600",
							"fontStyle": "italic",
							"fontFamily": "Bitter"
						}
					],
					"fontFamily": "\"Bitter\", sans-serif",
					"name": "Bitter",
					"slug": "bitter"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "18px",
					"slug": "small",
					"fluid": {
						"min": "17px",
						"max": "18px"
					}
				},
				{
					"name": "Medium",
					"size": "19px",
					"slug": "medium",
					"fluid": {
						"min": "18px",
						"max": "19px"
					}
				},
				{
					"name": "Large",
					"size": "32px",
					"slug": "large",
					"fluid": {
						"min": "24px",
						"max": "32px"
					}
				},
				{
					"name": "Extra Large",
					"size": "70px",
					"slug": "x-large",
					"fluid": {
						"min": "40px",
						"max": "70px"
					}
				},
				{
					"name": "2X Large",
					"size": "84px",
					"slug": "xx-large",
					"fluid": {
						"min": "44px",
						"max": "84px"
					}
				},
				{
					"name": "3X Large",
					"size": "140px",
					"slug": "xxx-large",
					"fluid": {
						"min": "40px",
						"max": "140px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontSize": "24px",
					"fontFamily": "var(--wp--preset--font-family--abel)",
					"fontWeight": "400",
					"lineHeight": {
						"ref": "styles.blocks.core/navigation.typography.lineHeight"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--abel)",
					"fontWeight": "400",
					"lineHeight": "1.3"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--abel)",
					"fontWeight": "400"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--abel)",
					"fontWeight": "400"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--abel)",
					"fontWeight": "400",
					"lineHeight": "1.2",
					"letterSpacing": "-0.02em"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "-0.01em",
					"fontWeight": "400"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "-0.01em",
					"fontWeight": "400"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "-0.01em"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "-0.01em"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ibarra-real-nova)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400",
			"lineHeight": "1.55",
			"letterSpacing": "0"
		}
	}
}