{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Fjalla & Roboto (Sans/Sans) (Bold)",
	"personality": [
		"bold"
	],
	"keywords": [
		"sans-serif",
		"display",
		"uppercase",
		"geometric",
		"dramatic",
		"strong",
		"assertive",
		"intense",
		"impact"
	],
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/roboto/Roboto_wdth_wght.woff2"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "italic",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/roboto/Roboto_wdth_wght_Italic.woff2"
							]
						}
					],
					"fontFamily": "\"Roboto\", sans-serif",
					"name": "Roboto",
					"slug": "roboto"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Fjalla One",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/fjalla-one/FjallaOne-Regular.woff2"
							]
						}
					],
					"fontFamily": "\"Fjalla One\", sans-serif",
					"name": "Fjalla One",
					"slug": "fjalla-one"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "18px",
					"slug": "small",
					"fluid": {
						"min": "17px",
						"max": "18px"
					}
				},
				{
					"name": "Medium",
					"size": "22px",
					"slug": "medium",
					"fluid": {
						"min": "20px",
						"max": "22px"
					}
				},
				{
					"name": "Large",
					"size": "32px",
					"slug": "large",
					"fluid": {
						"min": "28px",
						"max": "32px"
					}
				},
				{
					"name": "Extra Large",
					"size": "56px",
					"slug": "x-large",
					"fluid": {
						"min": "42px",
						"max": "60px"
					}
				},
				{
					"name": "2X Large",
					"size": "72px",
					"slug": "xx-large",
					"fluid": {
						"min": "48px",
						"max": "72px"
					}
				},
				{
					"name": "3X Large",
					"size": "160px",
					"slug": "xxx-large",
					"fluid": {
						"min": "72px",
						"max": "160px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--fjalla-one)",
					"fontWeight": "500",
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase",
					"lineHeight": "1.4",
					"letterSpacing": "0.05em"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"letterSpacing": "0.05em",
					"lineHeight": {
						"ref": "styles.blocks.core/navigation.typography.lineHeight"
					}
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "14px",
					"fontFamily": "var(--wp--preset--font-family--fjalla-one)",
					"fontWeight": "400",
					"textTransform": "uppercase",
					"letterSpacing": "0.05em"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--fjalla-one)",
					"fontWeight": "500",
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase",
					"lineHeight": "1.6",
					"letterSpacing": "0.05em"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--fjalla-one)",
					"fontWeight": "600",
					"textTransform": "uppercase",
					"lineHeight": "1"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1.1"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)",
			"fontWeight": "400",
			"fontSize": "var(--wp--preset--font-size--small)",
			"lineHeight": "1.6"
		}
	}
}