					"slug": "ibm-plex-mono"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "16px",
					"slug": "small",
					"fluid": {
						"min": "15px",
						"max": "16px"
					}
				},
				{
					"name": "Medium",
					"size": "17px",
					"slug": "medium",
					"fluid": {
						"min": "16px",
						"max": "17px"
					}
				},
				{
					"name": "Large",
					"size": "27px",
					"slug": "large",
					"fluid": {
						"min": "20px",
						"max": "27px"
					}
				},
				{
					"name": "Extra Large",
					"size": "56px",
					"slug": "x-large",
					"fluid": {
						"min": "40px",
						"max": "56px"
					}
				},
				{
					"name": "2X Large",
					"size": "72px",
					"slug": "xx-large",
					"fluid": {
						"min": "44px",
						"max": "72px"
					}
				},
				{
					"name": "3X Large",
					"size": "150px",
					"slug": "xxx-large",
					"fluid": {
						"min": "40px",
						"max": "150px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--ibm-plex-mono)",
					"fontWeight": "400",
					"letterSpacing": "normal",
					"textTransform": "uppercase",
					"lineHeight": {
						"ref": "styles.blocks.core/navigation.typography.lineHeight"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--ibm-plex-mono)",
					"fontWeight": "400",
					"letterSpacing": "normal",
					"textTransform": "uppercase",
					"lineHeight": "1.5"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--ibm-plex-mono)",
					"fontWeight": "400",
					"letterSpacing": "normal",
					"textTransform": "uppercase"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--ibm-plex-mono)",
					"fontWeight": "400",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--noto-serif)",
					"fontWeight": "200",
					"lineHeight": "1",
					"textTransform": "uppercase",
					"letterSpacing": "-0.04em"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"letterSpacing": "-0.02em",
					"lineHeight": "1.1"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontFamily": "var(--wp--preset--font-family--ibm-plex-mono)",
					"letterSpacing": "-0.02em"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"letterSpacing": "-0.02em"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"letterSpacing": "-0.02em"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"letterSpacing": "-0.02em"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ibm-plex-mono)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"lineHeight": "1.575",
			"letterSpacing": "-0.02em"
		}
	}
}