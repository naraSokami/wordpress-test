							"fontFamily": "Poppins"
						},
						{
							"src": "https://fonts.wp.com/s/poppins/v23/pxiByp8kv8JHgFVrLGT9V15vFP-KUEg.woff2",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontFamily": "Poppins"
						},
						{
							"src": "https://fonts.wp.com/s/poppins/v23/pxiDyp8kv8JHgFVrJJLmg1hlFd2PQEhcqw.woff2",
							"fontWeight": "500",
							"fontStyle": "italic",
							"fontFamily": "Poppins"
						},
						{
							"src": "https://fonts.wp.com/s/poppins/v23/pxiByp8kv8JHgFVrLEj6V15vFP-KUEg.woff2",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontFamily": "Poppins"
						},
						{
							"src": "https://fonts.wp.com/s/poppins/v23/pxiDyp8kv8JHgFVrJJLmr19lFd2PQEhcqw.woff2",
							"fontWeight": "600",
							"fontStyle": "italic",
							"fontFamily": "Poppins"
						}
					],
					"fontFamily": "\"Poppins\", sans-serif",
					"name": "Poppins",
					"slug": "poppins"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "16px",
					"slug": "small",
					"fluid": {
						"min": "15px",
						"max": "16px"
					}
				},
				{
					"name": "Medium",
					"size": "20px",
					"slug": "medium",
					"fluid": {
						"min": "18px",
						"max": "20px"
					}
				},
				{
					"name": "Large",
					"size": "32px",
					"slug": "large",
					"fluid": {
						"min": "22px",
						"max": "32px"
					}
				},
				{
					"name": "Extra Large",
					"size": "64px",
					"slug": "x-large",
					"fluid": {
						"min": "32px",
						"max": "64px"
					}
				},
				{
					"name": "2X Large",
					"size": "84px",
					"slug": "xx-large",
					"fluid": {
						"min": "34px",
						"max": "84px"
					}
				},
				{
					"name": "3X Large",
					"size": "120px",
					"slug": "xxx-large",
					"fluid": {
						"min": "36px",
						"max": "120px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontSize": "24px",
					"fontFamily": "var(--wp--preset--font-family--manrope)",
					"fontWeight": "400",
					"lineHeight": {
						"ref": "styles.blocks.core/navigation.typography.lineHeight"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontWeight": "400",
					"lineHeight": "1.3"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontWeight": "400"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--poppins)",
					"fontWeight": "400"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--manrope)",
					"fontWeight": "500",
					"lineHeight": "1.2",
					"letterSpacing": "-0.02em"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "400",
			"lineHeight": "1.6",
			"letterSpacing": "0"
		}
	}
}