{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Gabarito & Ibarra Real Nova (Sans/Serif) (Professional)",
	"personality": [
		"professional"
	],
	"keywords": [
		"monospace",
		"uppercase",
		"geometric",
		"dramatic",
		"tech",
		"edgy",
		"clean",
		"simple",
		"minimal"
	],
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Gabarito",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/gabarito/Gabarito-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "\"Gabarito\", sans-serif",
					"name": "Gabarito",
					"slug": "gabarito"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Ibarra Real Nova",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 800",
							"src": [
								"file:./assets/fonts/ibarra-real-nova/IbarraRealNova-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "\"Ibarra Real Nova\", serif",
					"name": "Ibarra Real Nova",
					"slug": "ibarra-real-nova"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "20px",
					"slug": "small",
					"fluid": {
						"min": "19px",
						"max": "20px"
					}
				},
				{
					"name": "Medium",
					"size": "18px",
					"slug": "medium",
					"fluid": {
						"min": "16px",
						"max": "18px"
					}
				},
				{
					"name": "Large",
					"size": "28px",
					"slug": "large",
					"fluid": {
						"min": "24px",
						"max": "28px"
					}
				},
				{
					"name": "Extra Large",
					"size": "54px",
					"slug": "x-large",
					"fluid": {
						"min": "40px",
						"max": "54px"
					}
				},
				{
					"name": "2X Large",
					"size": "64px",
					"slug": "xx-large",
					"fluid": {
						"min": "44px",
						"max": "64px"
					}
				},
				{
					"name": "3X Large",
					"size": "140px",
					"slug": "xxx-large",
					"fluid": {
						"min": "40px",
						"max": "140px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gabarito)",
					"fontSize": "14px",
					"fontWeight": "500",
					"letterSpacing": "1px",
					"textTransform": "uppercase",
					"lineHeight": {
						"ref": "styles.blocks.core/navigation.typography.lineHeight"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gabarito)",
					"fontSize": "14px",
					"fontWeight": "500",
					"letterSpacing": "1px",
					"textTransform": "uppercase",
					"lineHeight": "1.4"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "14px",
					"fontFamily": "var(--wp--preset--font-family--gabarito)",
					"fontWeight": "400",
					"letterSpacing": "1px",
					"textTransform": "uppercase"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontSize": "14px",
					"fontFamily": "var(--wp--preset--font-family--gabarito)",
					"fontWeight": "500",
					"letterSpacing": "1px",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--gabarito)",
					"fontWeight": "600",
					"lineHeight": "0.975",
					"textTransform": "uppercase",
					"letterSpacing": "0.025em"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ibarra-real-nova)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"lineHeight": "1.4",
			"letterSpacing": "-0.01em"
		}
	}
}