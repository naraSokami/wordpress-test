	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontSize": "24px",
					"fontFamily": "var(--wp--preset--font-family--familjen-grotesk)",
					"fontWeight": "400",
					"lineHeight": {
						"ref": "styles.blocks.core/navigation.typography.lineHeight"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--arimo)",
					"fontWeight": "400",
					"lineHeight": "1.4"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--arimo)",
					"fontWeight": "400"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--arimo)",
					"fontWeight": "400"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--familjen-grotesk)",
					"fontWeight": "400",
					"lineHeight": "1.2",
					"letterSpacing": "-0.02em"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "-0.01em",
					"fontWeight": "400"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "-0.01em",
					"fontWeight": "400"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "-0.01em"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "-0.01em"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--arimo)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400",
			"lineHeight": "1.55",
			"letterSpacing": "0"
		}
	}
}