{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Lucette & DM Mono (Serif/Mono) (Sophisticated)",
	"personality": [
		"sophisticated"
	],
	"keywords": [
		"serif",
		"monospace",
		"edgy",
		"tech",
		"editorial",
		"minimal",
		"futuristic",
		"expressive",
		"didone",
		"industrial",
		"modern"
	],
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Lucette",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/lucette/Lucette-Regular.woff2"
							]
						},
						{
							"fontFamily": "Lucette",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/lucette/Lucette-Regularitalic.woff2"
							]
						}
					],
					"fontFamily": "\"Lucette\", sans-serif",
					"name": "Lucette",
					"slug": "lucette"
				},
				{
					"fontFace": [
						{
							"fontFamily": "DM Mono",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/dm-mono/DMMono-Light.woff2"
							]
						},
						{
							"fontFamily": "DM Mono",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/dm-mono/DMMono-LightItalic.woff2"
							]
						},
						{
							"fontFamily": "DM Mono",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/dm-mono/DMMono-Regular.woff2"
							]
						},
						{
							"fontFamily": "DM Mono",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/dm-mono/DMMono-Italic.woff2"
							]
						},
						{
							"fontFamily": "DM Mono",
							"fontStyle": "normal",
							"fontWeight": "500 600",
							"src": [
								"file:./assets/fonts/dm-mono/DMMono-Medium.woff2"
							]
						},
						{
							"fontFamily": "DM Mono",
							"fontStyle": "italic",
							"fontWeight": "500 600",
							"src": [
								"file:./assets/fonts/dm-mono/DMMono-MediumItalic.woff2"
							]
						}
					],
					"fontFamily": "\"DM Mono\", monospace",
					"name": "DM Mono",
					"slug": "dm-mono"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "15px",
					"slug": "small",
					"fluid": {
						"min": "14px",
						"max": "15px"
					}
				},
				{
					"name": "Medium",
					"size": "17px",
					"slug": "medium",
					"fluid": {
						"min": "16px",
						"max": "17px"
					}
				},
				{
					"name": "Large",
					"size": "24px",
					"slug": "large",
					"fluid": {
						"min": "20px",
						"max": "24px"
					}
				},
				{
					"name": "Extra Large",
					"size": "62px",
					"slug": "x-large",
					"fluid": {
						"min": "36px",
						"max": "62px"
					}
				},
				{
					"name": "2X Large",
					"size": "72px",
					"slug": "xx-large",
					"fluid": {
						"min": "40px",
						"max": "72px"
					}
				},
				{
					"name": "3X Large",
					"size": "130px",
					"slug": "xxx-large",
					"fluid": {
						"min": "46px",
						"max": "130px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--dm-mono)",
					"fontWeight": "300",
					"lineHeight": {
						"ref": "styles.blocks.core/navigation.typography.lineHeight"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--dm-mono)",
					"fontWeight": "300",
					"lineHeight": "1.5"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontFamily": "var(--wp--preset--font-family--dm-mono)",
					"fontWeight": "300"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-mono)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lucette)",
					"fontWeight": "normal",
					"letterSpacing": "-0.02em"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--dm-mono)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "-0.04em"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "normal"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "normal"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "normal"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--dm-mono)",
			"lineHeight": "1.65",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontWeight": "400"
		}
	}
}