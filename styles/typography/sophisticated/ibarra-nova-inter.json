{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Ibarra Real Nova & Inter (Serif/Sans) (Sophisticated)",
	"personality": [
		"sophisticated"
	],
	"keywords": [
		"serif",
		"elegant",
		"light",
		"sharp",
		"editorial",
		"classic",
		"fashion"
	],
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Ibarra Real Nova",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 800",
							"src": [
								"file:./assets/fonts/ibarra-real-nova/IbarraRealNova-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "\"Ibarra Real Nova\", serif",
					"name": "Ibarra Real Nova",
					"slug": "ibarra-real-nova"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 800",
							"src": [
								"file:./assets/fonts/inter/InterVariable.woff2"
							]
						},
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "100 800",
							"src": [
								"file:./assets/fonts/inter/InterVariable-Italic.woff2"
							]
						}
					],
					"fontFamily": "\"Inter\", sans-serif",
					"name": "Inter",
					"slug": "inter"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "19px",
					"slug": "small",
					"fluid": {
						"min": "18px",
						"max": "19px"
					}
				},
				{
					"name": "Medium",
					"size": "20px",
					"slug": "medium",
					"fluid": {
						"min": "18px",
						"max": "20px"
					}
				},
				{
					"name": "Large",
					"size": "26px",
					"slug": "large",
					"fluid": {
						"min": "18px",
						"max": "26px"
					}
				},
				{
					"name": "Extra Large",
					"size": "64px",
					"slug": "x-large",
					"fluid": {
						"min": "32px",
						"max": "64px"
					}
				},
				{
					"name": "2X Large",
					"size": "84px",
					"slug": "xx-large",
					"fluid": {
						"min": "34px",
						"max": "84px"
					}
				},
				{
					"name": "3X Large",
					"size": "120px",
					"slug": "xxx-large",
					"fluid": {
						"min": "36px",
						"max": "120px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "13px",
					"fontWeight": "600",
					"textTransform": "uppercase",
					"letterSpacing": "3px",
					"lineHeight": {
						"ref": "styles.blocks.core/navigation.typography.lineHeight"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "13px",
					"fontWeight": "500",
					"textTransform": "uppercase",
					"letterSpacing": "3px",
					"lineHeight": "1.4"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "12px",
					"fontWeight": "500",
					"textTransform": "uppercase",
					"letterSpacing": "3px"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "13px",
					"fontWeight": "500",
					"textTransform": "uppercase",
					"letterSpacing": "3px"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--ibarra-real-nova)",
					"fontWeight": "100",
					"lineHeight": "1",
					"letterSpacing": "-0.01em"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "0.9"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
                    "fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "var(--wp--preset--font-size--large)",
                    "textTransform": "uppercase",
					"letterSpacing": "3px",
                    "fontWeight": "500"
				}
			},
			"h4": {
				"typography": {
                    "fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "var(--wp--preset--font-size--medium)",
                    "textTransform": "uppercase",
					"letterSpacing": "3px",
                    "fontWeight": "500"
				}
			},
			"h5": {
				"typography": {
                    "fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "500",
                    "letterSpacing": "2px"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
                     "fontWeight": "500"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--ibarra-real-nova)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"lineHeight": "1.55em"
		}
	}
}