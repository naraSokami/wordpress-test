{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Calistoga & Bitter (Serif/Serif) (Playful)",
	"personality": [
		"playful"
	],
	"keywords": [
		"serif",
		"chunky",
		"display",
		"slab",
		"retro",
		"expressive",
		"creative",
		"magazine",
		"newspaper",
		"book",
		"editorial"
	],
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"src": "https://fonts.wp.com/s/calistoga/v16/6NUU8F2OJg6MeR7l4e0vscYAwdRZfw.woff2",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontFamily": "Calistoga"
						}
					],
					"fontFamily": "\"Calistoga\", sans-serif",
					"name": "Calistoga",
					"slug": "calistoga"
				},
				{
					"fontFace": [
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxhHiqOu8IVPmnRc6SY1KXhnF_Y8ajfCLrEXFh2reU.woff2",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxhHiqOu8IVPmnRc6SY1KXhnF_Y8fbfCLrEXFh2reU.woff2",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxhHiqOu8IVPmnRc6SY1KXhnF_Y8cTfCLrEXFh2reU.woff2",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxhHiqOu8IVPmnRc6SY1KXhnF_Y8SjYCLrEXFh2reU.woff2",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxjHiqOu8IVPmn7epZnDMyKBvHf5D6cvvzOXXpzveWxBw.woff2",
							"fontWeight": "300",
							"fontStyle": "italic",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxjHiqOu8IVPmn7epZnDMyKBvHf5D6c4PzOXXpzveWxBw.woff2",
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxjHiqOu8IVPmn7epZnDMyKBvHf5D6c0vzOXXpzveWxBw.woff2",
							"fontWeight": "500",
							"fontStyle": "italic",
							"fontFamily": "Bitter"
						},
						{
							"src": "https://fonts.wp.com/s/bitter/v39/raxjHiqOu8IVPmn7epZnDMyKBvHf5D6cPvvOXXpzveWxBw.woff2",
							"fontWeight": "600",
							"fontStyle": "italic",
							"fontFamily": "Bitter"
						}
					],
					"fontFamily": "\"Bitter\", sans-serif",
					"name": "Bitter",
					"slug": "bitter"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "16px",
					"slug": "small",
					"fluid": {
						"min": "16px",
						"max": "16px"
					}
				},
				{
					"name": "Medium",
					"size": "17px",
					"slug": "medium",
					"fluid": {
						"min": "16px",
						"max": "17px"
					}
				},
				{
					"name": "Large",
					"size": "28px",
					"slug": "large",
					"fluid": {
						"min": "22px",
						"max": "28px"
					}
				},
				{
					"name": "Extra Large",
					"size": "56px",
					"slug": "x-large",
					"fluid": {
						"min": "40px",
						"max": "56px"
					}
				},
				{
					"name": "2X Large",
					"size": "70px",
					"slug": "xx-large",
					"fluid": {
						"min": "44px",
						"max": "70px"
					}
				},
				{
					"name": "3X Large",
					"size": "116px",
					"slug": "xxx-large",
					"fluid": {
						"min": "36px",
						"max": "116px"
					}
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontSize": "24px",
					"fontFamily": "var(--wp--preset--font-family--bitter)",
					"fontWeight": "400",
					"lineHeight": {
						"ref": "styles.blocks.core/navigation.typography.lineHeight"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--bitter)",
					"fontWeight": "400",
					"lineHeight": "1.5"
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--bitter)",
					"fontWeight": "400"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontFamily": "var(--wp--preset--font-family--bitter)",
					"fontWeight": "400"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--calistoga)",
					"fontWeight": "400",
					"lineHeight": "1.2",
					"letterSpacing": "0"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "1.1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.2",
					"fontWeight": "400"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.2",
					"fontWeight": "400"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.2"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.2"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--bitter)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400",
			"lineHeight": "1.6",
			"letterSpacing": "0"
		}
	}
}