{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Urbanist & Poppins (Sans/Sans) (Sophisticated)",
	"personality": [
		"sophisticated"
	],
	"keywords": [
		"sans-serif",
		"geometric",
		"rounded",
		"modern",
		"clean",
		"neutral",
		"contemporary",
		"light"
	],
